within AixLib.Media;
package Antifreeze "Medium packages of secondary working fluids"
  extends Modelica.Icons.Package;

  annotation (preferredView="info", Documentation(info="<html><p>
  This package contains media models for liquid mixtures used in HVAC
  applications. The media models evaluate the thermophysical properties
  at a user-specifiable temperature <code>property_T</code>, and the
  thermophysical properties are then kept constant during the
  simulation.
</p>
</html>"));
end Antifreeze;
